{
  "manifest_version": 3,
  "name": "Leetcode Dashboard",
  "version": "1.0.0",
  "action":{
    "default_popup": "index.html",
    "default_title": "dashboard"
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/*"
      ],
      "js": ["content.js", "speech.js"]
    }
  ],
  "permissions": [
    "scripting"
  ]
}
